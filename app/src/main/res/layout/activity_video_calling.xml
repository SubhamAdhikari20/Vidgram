<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.activity.VideoCallingActivity">

    <Button
        android:id="@+id/Back_Btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="405dp"
        android:layout_height="809dp"
        app:srcCompat="@drawable/authentication_btn"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="84dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="135dp"
        android:layout_height="192dp"
        app:srcCompat="@drawable/authentication_text_field"
        tools:layout_editor_absoluteX="260dp"
        tools:layout_editor_absoluteY="108dp" />

    <EditText
        android:id="@+id/Ans_Dec_Btn"
        android:layout_width="294dp"
        android:layout_height="57dp"
        android:ems="10"
        android:inputType="text"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="781dp" />

    <Button
        android:id="@+id/Video_Btn"
        android:layout_width="63dp"
        android:layout_height="59dp"
        android:text="Video"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="761dp" />

    <Button
        android:id="@+id/Mic_Btn"
        android:layout_width="63dp"
        android:layout_height="59dp"
        android:text="Microphone"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="761dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="63dp"
        android:layout_height="59dp"
        android:text="Button"
        tools:layout_editor_absoluteX="284dp"
        tools:layout_editor_absoluteY="761dp" />
</androidx.constraintlayout.widget.ConstraintLayout>